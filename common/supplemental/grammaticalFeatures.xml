  </grammaticalFeatures>
  <grammaticalFeatures targets="nominal" locales="tr">
    <grammaticalFeatureValues type="case" value="nominative genitive dative accusative instrumental ablative locative"/>
  </grammaticalFeatures>
  <grammaticalFeatures targets="nominal" locales="id">
    <grammaticalFeatureValues type="definiteness" value="definite indefinite"/>
